../assets/serviceSystem/4.jpg';\nimport serve5 from '../assets/serviceSystem/5.jpg';\nimport serve6 from '../assets/serviceSystem/6.jpg';\nimport serve7 from '../assets/serviceSystem/7.jpg';\nimport serve8 from '../assets/serviceSystem/8.jpg';\nexport default {\n  name: \"\",\n  components: {},\n  props: [\"\"],\n\n  data() {\n    return {\n      bannerData: [{\n        img: banner\n      }, {\n        img: banner1\n      }, {\n        img: banner2\n      }, {\n        img: banner3\n      }],\n      imgs: [[{\n        imgItem: con1\n      }, {\n        imgItem: con2\n      }, {\n        imgItem: con3\n      }, {\n        imgItem: con4\n      }], [{\n        imgItem: sec1\n      }, {\n        imgItem: sec2\n      }, {\n        imgItem: sec3\n      }, {\n        imgItem: sec4\n      }], [{\n        imgItem: old1\n      }, {\n        imgItem: old2\n      }, {\n        imgItem: old3\n      }, {\n        imgItem: old4\n      }], [{\n        imgItem: hou1\n      }, {\n        imgItem: hou2\n      }, {\n        imgItem: hou3\n      }, {\n        imgItem: hou4\n      }]],\n      selfCare: [self1, self2, self3, self4],\n      serveSys: [serve1, serve2, serve3, serve4, serve5, serve6, serve7, serve8],\n      btnIndex: 0,\n      handleIndex: 0,\n      designSelet: [\"社区配套\", \"景观\", \"舒适化\", \"户型\"],\n      tags: [\"自理生活\", \"协助生活\", \"专业护理\", \"记忆障碍\"]\n    };\n  },\n\n  watch: {},\n\n  created() {},\n\n  beforeMount() {},\n\n  mounted() {},\n\n  methods: {\n    selectTab(index) {\n      this.btnIndex = index;\n    },\n\n    handleSelect(index) {\n      this.handleIndex = index;\n    },\n\n    classIcon(index) {\n      return `icoin_${index}`;\n    }\n\n  },\n  computed: {}\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgHA;AACA;AACA;AACA;AACA,2D,CAEA;;AACA;AACA;AACA;AACA,uD,CAEA;;AACA;AACA;AACA;AACA,qD,CAEA;;AACA;AACA;AACA;AACA,iD,CAEA;;AACA;AACA;AACA;AACA,mD,CAEA;;AACA;AACA;AACA;AACA,6C,CAGA;;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAGA;EACAA,QADA;EAEAC,cAFA;EAGAC,WAHA;;EAIAC;IACA;MACAC,aACA;QAAAC;MAAA,CADA,EAEA;QAAAA;MAAA,CAFA,EAGA;QAAAA;MAAA,CAHA,EAIA;QAAAA;MAAA,CAJA,CADA;MAOAC,OACA,CACA;QAAAC;MAAA,CADA,EAEA;QAAAA;MAAA,CAFA,EAGA;QAAAA;MAAA,CAHA,EAIA;QAAAA;MAAA,CAJA,CADA,EAOA,CACA;QAAAA;MAAA,CADA,EAEA;QAAAA;MAAA,CAFA,EAGA;QAAAA;MAAA,CAHA,EAIA;QAAAA;MAAA,CAJA,CAPA,EAaA,CACA;QAAAA;MAAA,CADA,EAEA;QAAAA;MAAA,CAFA,EAGA;QAAAA;MAAA,CAHA,EAIA;QAAAA;MAAA,CAJA,CAbA,EAmBA,CACA;QAAAA;MAAA,CADA,EAEA;QAAAA;MAAA,CAFA,EAGA;QAAAA;MAAA,CAHA,EAIA;QAAAA;MAAA,CAJA,CAnBA,CAPA;MAiCAC,WACAC,KADA,EACAC,KADA,EACAC,KADA,EACAC,KADA,CAjCA;MAoCAC,0EApCA;MAqCAC,WArCA;MAsCAC,cAtCA;MAuCAC,wCAvCA;MAwCAC;IAxCA;EA0CA,CA/CA;;EAgDAC,SAhDA;;EAiDAC,YAjDA;;EAkDAC,gBAlDA;;EAmDAC,YAnDA;;EAoDAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;IACA,CANA;;IAOAC;MACA;IACA;;EATA,CApDA;EA+DAC;AA/DA","names":["name","components","props","data","bannerData","img","imgs","imgItem","selfCare","self1","self2","self3","self4","serveSys","btnIndex","handleIndex","designSelet","tags","watch","created","beforeMount","mounted","methods","selectTab","handleSelect","classIcon","computed"],"sourceRoot":"src/views","sources":["index.vue"],"sourcesContent":["<template>\r\n  <div class=\"Index\">\r\n    <!-- 轮播 -->\r\n    <div class=\"block\">\r\n      <el-carousel trigger=\"click\" height=\"596px\">\r\n        <el-carousel-item v-for=\"(item, index) in bannerData\" :key=\"index\">\r\n          <img class=\"banner-img\" :src=\"item.img\" alt=\"\">\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n    </div>\r\n    <!-- 个性化设计模块 -->\r\n    <div class=\"design\">\r\n      <div class=\"desc\">\r\n         <span class=\"add-color\">舒适的</span>\r\n         <span class=\"default-style\">空间尺度+</span>\r\n         <span class=\"add-color\">人性化的</span>\r\n         <span class=\"default-style\">规划设计+</span>\r\n         <span class=\"add-color\">高端的</span>\r\n         <span class=\"default-style\">专业服务</span>\r\n      </div>\r\n      <div class=\"btn-group\">\r\n        <button :class = \"{active:!(index-btnIndex)}\" @click=\"selectTab(index)\" v-for=\"(btnName,index) in designSelet\" :key=\"index\">{{btnName}}</button>\r\n      </div>\r\n      <div class=\"content-wrap\">\r\n           <ul v-for=\"(item, index) in imgs\" class=\"item\" :key=\"index\"  v-show = 'btnIndex == index'>\r\n             <li v-for=\"(img, keys) in item\"  :key=\"keys\">\r\n               <img :src=\"img.imgItem\" alt=\"\" />\r\n             </li>\r\n           </ul>\r\n      </div>\r\n    </div>\r\n    <!-- 全面服务 -->\r\n    <div class=\"all-serve\">\r\n       <div>\r\n          <span class=\"add-color\">全面服务</span>\r\n          <span class=\"default-style\">涵盖老年生活四大业态</span>\r\n       </div>\r\n       <div class=\"serve-img\" v-for=\"(item, index) in selfCare\"  :key=\"index\" v-show = 'handleIndex == index'>\r\n         <img :src=\"item\" alt=\"\">\r\n       </div>\r\n       <div class=\"item-box\">\r\n         <div v-for=\"(tag,index) in tags\" :key=\"tag\" @click=\"handleSelect(index)\" class=\"tab-item\">\r\n            <i :class = \"classIcon(index)\"></i>\r\n            <span :class = \"{selectStyle:!(index-handleIndex)}\">{{tag}}</span>\r\n         </div>\r\n       </div>\r\n    </div>\r\n    <!-- 七大服务体系 -->\r\n    <div class=\"serve-sys\">\r\n       <div>\r\n          <span class=\"add-color\">八大</span>\r\n          <span class=\"default-style\">服务体系</span>\r\n       </div>\r\n       <p class=\"sys-desc\">铭佳康养社区为老年人打造一个全新的精神文化生活圈：打破老年人的被动孤立，让老年人生活在社会中，互通、互助，融入社会，拥有社会依存感，和孤独绝缘，与快乐作伴。\r\n       </p>\r\n        <p class=\"sys-desc\">创造温馨的家、开放的大学、优雅的活力中心、高品质医疗保健中心、长辈精神和心灵的家园，创造长辈的“五位一体”养老生活方式。</p>\r\n       <!-- <p class=\"sys-desc\">康养社区为老年人打造一个全新的精神文化生活圈：打破老年人的被动孤立，让老年人生活在社会中，互通、互助，融入社会，拥有社会依存感，和孤独绝缘，与快乐作伴</p> -->\r\n       <div class=\"sys-imgs\">\r\n         <img v-for=\"(item, index) in serveSys\" :key=\"index\"  :src=\"item\" alt=\"\">\r\n       </div>\r\n    </div>\r\n    <!-- 医社保 -->\r\n    <div class=\"insurance\">\r\n      <div class=\"desc\">\r\n         <span class=\"add-color\">医保</span>\r\n         <span class=\"default-style\">接入</span>\r\n         <span class=\"add-color\">+全面</span>\r\n         <span class=\"default-style\">健康服务</span>\r\n         <span class=\"add-color\">+专业</span>\r\n         <span class=\"default-style\">服务团队</span>\r\n         <span class=\"add-color\">+管家式</span>\r\n         <span class=\"default-style\">服务</span>\r\n      </div>\r\n      <div class=\"img-text-wrap\">\r\n        <div class=\"left-box\">\r\n          <div class=\"top\">\r\n             <div class=\"text-wrap\">\r\n                <h3>康复医院、护理院均接入医保</h3>\r\n                <p>智慧康养社区老人在康复医院看病可用医保</p>\r\n              </div>\r\n              <img  src=\"../assets/in_btm1.png\" alt=\"\">\r\n          </div>\r\n          <div class=\"top\">\r\n             <div class=\"text-wrap\">\r\n                <h3>全面多维度健康服务模式</h3>\r\n                <p>社区配套两家医院，针对长者推出“私人健康管理师”，为长者制定专业的健康管理方案</p>\r\n              </div>\r\n              <img src=\"../assets/in_btm2.png\" alt=\"\">\r\n              </div>\r\n        </div>\r\n         <div style=\"margin-top:20px\" class=\"right-box\">\r\n          <div class=\"top\">\r\n             <div class=\"text-wrap\">\r\n                <h3>专业高素质的服务管理团队</h3>\r\n                <p>康养社区设有专业人才培训部门，结合学院的人才输送，将为康养社区提供完善的人才保障</p>\r\n              </div>\r\n              <img  src=\"../assets/in_btm3.png\" alt=\"\">\r\n          </div>\r\n          <div class=\"top\">\r\n             <div class=\"text-wrap\">\r\n                <h3>管家式个性化的服务模式</h3>\r\n                <p>社区使用专业评估工具对长者身体状况、兴趣爱好、饮食习惯等评估，并根据结果为其提供个性化服务</p>\r\n              </div>\r\n              <img src=\"../assets/in_btm4.png\" alt=\"\">\r\n              </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\n// 户型\r\nimport hou1 from '../assets/landscape/HouseType/alone.png'\r\nimport hou2 from '../assets/landscape/HouseType/huli.png'\r\nimport hou3 from '../assets/landscape/HouseType/recover.png'\r\nimport hou4 from '../assets/landscape/HouseType/dubou.png'\r\n\r\n// 社区\r\nimport con1 from '../assets/landscape/community/1.png'\r\nimport con2 from '../assets/landscape/community/2.png'\r\nimport con3 from '../assets/landscape/community/3.png'\r\nimport con4 from '../assets/landscape/community/4.png'\r\n\r\n// 景观\r\nimport sec1 from '../assets/landscape/scenery/1.png'\r\nimport sec2 from '../assets/landscape/scenery/2.png'\r\nimport sec3 from '../assets/landscape/scenery/3.png'\r\nimport sec4 from '../assets/landscape/scenery/4.png'\r\n\r\n// 适老化\r\nimport old1 from '../assets/landscape/old/1.png'\r\nimport old2 from '../assets/landscape/old/2.png'\r\nimport old3 from '../assets/landscape/old/3.png'\r\nimport old4 from '../assets/landscape/old/4.png'\r\n\r\n// banner\r\nimport banner from '../assets/banner/banner.jpg'\r\nimport banner1 from '../assets/banner/banner1.jpg'\r\nimport banner2 from '../assets/banner/banner2.jpg'\r\nimport banner3 from '../assets/banner/banner3.jpg'\r\n\r\n// 自理生活\r\nimport self1 from '../assets/selfCare/1.jpg'\r\nimport self2 from '../assets/selfCare/2.jpg'\r\nimport self3 from '../assets/selfCare/3.jpg'\r\nimport self4 from '../assets/selfCare/4.jpg'\r\n\r\n\r\n// 八大服务体系\r\nimport serve1 from '../assets/serviceSystem/1.jpg'\r\nimport serve2 from '../assets/serviceSystem/2.jpg'\r\nimport serve3 from '../assets/serviceSystem/3.jpg'\r\nimport serve4 from '../assets/serviceSystem/4.jpg'\r\nimport serve5 from '../assets/serviceSystem/5.jpg'\r\nimport serve6 from '../assets/serviceSystem/6.jpg'\r\nimport serve7 from '../assets/serviceSystem/7.jpg'\r\nimport serve8 from '../assets/serviceSystem/8.jpg'\r\n\r\n\r\nexport default {\r\n  name: \"\",\r\n  components: {},\r\n  props: [\"\"],\r\n  data() {\r\n    return {\r\n      bannerData: [\r\n        {img: banner},\r\n        {img: banner1},\r\n        {img: banner2},\r\n        {img: banner3}\r\n      ],\r\n      imgs: [\r\n        [\r\n          {imgItem: con1},\r\n          {imgItem: con2},\r\n          {imgItem: con3},\r\n          {imgItem: con4},\r\n        ],\r\n        [\r\n          {imgItem: sec1 },\r\n          {imgItem: sec2 },\r\n          {imgItem: sec3 },\r\n          {imgItem: sec4 },\r\n        ],\r\n        [\r\n          {imgItem: old1 },\r\n          {imgItem: old2 },\r\n          {imgItem: old3 },\r\n          {imgItem: old4 },\r\n        ],\r\n        [\r\n          {imgItem: hou1 },\r\n          {imgItem: hou2 },\r\n          {imgItem: hou3 },\r\n          {imgItem: hou4 },\r\n        ]\r\n      ],\r\n      selfCare: [\r\n        self1, self2, self3, self4\r\n      ],\r\n      serveSys:[serve1,serve2,serve3,serve4,serve5,serve6,serve7,serve8],\r\n      btnIndex: 0,\r\n      handleIndex: 0,\r\n      designSelet: [\"社区配套\", \"景观\", \"舒适化\", \"户型\"],\r\n      tags: [\"自理生活\", \"协助生活\", \"专业护理\", \"记忆障碍\"]\r\n    };\r\n  },\r\n  watch: {},\r\n  created() {},\r\n  beforeMount() {},\r\n  mounted() {},\r\n  methods: {\r\n    selectTab(index) {\r\n      this.btnIndex = index;\r\n    },\r\n    handleSelect(index) {\r\n      this.handleIndex = index;\r\n    },\r\n    classIcon(index) {\r\n      return `icoin_${index}`;\r\n    }\r\n  },\r\n  computed: {}\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n.content .Index{\r\n  width: 1440px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.el-carousel__item h3 {\r\n  color: #475669;\r\n  font-size: 14px;\r\n  opacity: 0.75;\r\n  line-height: 596px;\r\n  margin: 0;\r\n}\r\n.el-carousel__item:nth-child(2n) {\r\n  background-color: #99a9bf;\r\n}\r\n.el-carousel__item:nth-child(2n + 1) {\r\n  background-color: #d3dce6;\r\n}\r\n.banner-img {\r\n  width: 100%;\r\n  height: 596px;\r\n}\r\n\r\n/* 个性化设计 */\r\n.design {\r\n  margin-top: 40px;\r\n}\r\n\r\n.add-color {\r\n  color: #1a3e79;\r\n  font-size: 40px;\r\n  font-weight: bolder;\r\n}\r\n.default-style {\r\n  color: #333;\r\n  font-size: 36px;\r\n  font-weight: bolder;\r\n}\r\n.btn-group {\r\n  display: flex;\r\n  width: 33%;\r\n  margin: 40px auto 0;\r\n  justify-content: space-between;\r\n}\r\n\r\n.btn-group button {\r\n  width: 100px;\r\n  height: 30px;\r\n  line-height: 30px;\r\n  border-radius: 5px;\r\n  border: none;\r\n  outline: none;\r\n}\r\n.btn-group button:hover {\r\n  cursor: pointer;\r\n}\r\n.active {\r\n  background: #1a3e79;\r\n  color: #fff;\r\n}\r\n.content-wrap {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  margin-top: 36px;\r\n}\r\n.item {\r\n  display: flex;\r\n  box-sizing: border-box;\r\n  justify-content: center;\r\n  padding: 0 60px;\r\n}\r\n.item li {\r\n  width: 280px;\r\n  height: 400px;\r\n  margin-right: 26px;\r\n}\r\n.item li img {\r\n  width: 100%;\r\n  height: 100%;\r\n  border-radius: 5px;\r\n}\r\n\r\n/* 全面服务 */\r\n.all-serve {\r\n  margin-top: 90px;\r\n}\r\n\r\n.serve-img img {\r\n  width: 100%;\r\n  height: 460px;\r\n  margin-top: 45px;\r\n  vertical-align: bottom;\r\n}\r\n.item-box {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  box-sizing: border-box;\r\n  padding: 0 330px;\r\n  margin-top: 21px;\r\n}\r\n.tab-item:hover {\r\n  cursor: pointer;\r\n}\r\n.selectStyle {\r\n  color: #1a3e79;\r\n}\r\n.tab-item {\r\n  display: flex;\r\n  align-items:flex-end;\r\n}\r\n.tab-item i {\r\n  display: inline-block;\r\n  margin-right: 12px;\r\n}\r\n\r\n.icoin_0{\r\n  width: 24px;\r\n  height: 32px;\r\n  background: url(\"../assets/icon1.png\") no-repeat;\r\n}\r\n\r\n.icoin_1{\r\n  width: 31px;\r\n  height: 36px;\r\n  background: url(\"../assets/icon2.png\") no-repeat;\r\n}\r\n\r\n.icoin_2{\r\n  width: 31px;\r\n  height: 25px;\r\n  background: url(\"../assets/icon3.png\") no-repeat;\r\n}\r\n\r\n.icoin_3{\r\n  width: 24px;\r\n  height: 28px;\r\n  display: block;\r\n  background: url(\"../assets/icon4.png\") no-repeat;\r\n}\r\n\r\n/* 七大服务体系 */\r\n.serve-sys {\r\n  margin-top: 90px;\r\n}\r\n.sys-desc {\r\n  box-sizing: border-box;\r\n  padding: 0 120px;\r\n  color: #333;\r\n  line-height: 40px;\r\n}\r\n.sys-imgs {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: space-between;\r\n  box-sizing: border-box;\r\n  padding: 0 106px;\r\n  margin-top: 30px;\r\n}\r\n.sys-imgs img {\r\n  width: 292px;\r\n  height: 292px;\r\n  /* margin-right: 20px; */\r\n  margin-bottom: 20px;\r\n  border-radius: 2px;\r\n  vertical-align: bottom;\r\n}\r\n\r\n/* 医社保 */\r\n.insurance {\r\n  margin-top: 90px;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n}\r\n.img-text-wrap {\r\n  margin-top: 45px;\r\n  display: flex;\r\n  box-sizing: border-box;\r\n  flex-direction: column;\r\n  margin-bottom: 80px;\r\n}\r\n.top{\r\n  display: flex;\r\n  width: 580px;\r\n  height: 240px;\r\n  margin-right: 20px;\r\n}\r\n.text-wrap{\r\n  background: #fff;\r\n  box-sizing: border-box;\r\n  padding:50px 20px 20px;\r\n}\r\n.text-wrap h3,\r\n.text-wrap p {\r\n  margin: 0;\r\n  text-align: left;\r\n}\r\n.left-box,\r\n.right-box {\r\n  display: flex;\r\n  box-sizing: border-box;\r\n  padding: 0 95px;\r\n}\r\n.left-box img,\r\n.right-box img {\r\n  width: 240px;\r\n  height: 240px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}